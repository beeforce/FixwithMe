<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/myrl"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/resbt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="60dp"
        android:layout_marginStart="69dp"
        android:onClick="OnReg"
        android:text="Confirm"
        android:textColor="@android:color/holo_red_dark"
        tools:layout_editor_absoluteX="148dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/button2" />

    <EditText
        android:id="@+id/et_username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/resbt"
        android:layout_marginStart="19dp"
        android:layout_marginTop="28dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="12"
        android:textSize="14sp" />

    <EditText
        android:id="@+id/et_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/et_username"
        android:layout_below="@+id/et_username"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="10"
        android:textSize="14sp" />

    <EditText
        android:id="@+id/et_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/et_password"
        android:layout_below="@+id/et_password"
        android:ems="10"
        android:inputType="phone"
        android:maxLength="10"
        android:textSize="14sp" />

    <EditText
        android:id="@+id/et_first"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/et_phone"
        android:layout_below="@+id/et_phone"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="12"
        android:textSize="14sp" />

    <EditText
        android:id="@+id/et_lastname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/et_first"
        android:layout_below="@+id/et_first"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="12"
        android:textSize="14sp" />

    <EditText
        android:id="@+id/et_garage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/et_lastname"
        android:layout_below="@+id/et_lastname"
        android:ems="10"
        android:inputType="textPersonName"
        android:maxLength="12"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/U"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/et_username"
        android:layout_toStartOf="@+id/et_password"
        android:text="Username"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_username"
        android:layout_toStartOf="@+id/et_password"
        android:text="Password"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_password"
        android:layout_toStartOf="@+id/et_phone"
        android:text="Phonenumber"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_phone"
        android:layout_toStartOf="@+id/et_first"
        android:text="Firstname"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/U"
        android:layout_below="@+id/et_first"
        android:text="Lastname"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et_lastname"
        android:layout_toStartOf="@+id/et_garage"
        android:text="Garagename"
        android:textColor="@color/colorPrimaryDark" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/et_address"
        android:layout_marginTop="17dp"
        android:layout_toStartOf="@+id/et_address"
        android:text="Address"
        android:textColor="@color/colorPrimaryDark" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/U"
        android:layout_alignParentStart="true"
        android:onClick="Backtolog"
        android:text="Back"
        android:textSize="12sp" />

    <EditText
        android:id="@+id/et_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/et_garage"
        android:layout_below="@+id/et_garage"
        android:layout_toEndOf="@+id/textView11"
        android:ems="10"
        android:inputType="textMultiLine"
        android:textSize="14sp" />
</RelativeLayout>
